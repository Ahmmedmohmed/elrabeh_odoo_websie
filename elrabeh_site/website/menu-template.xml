        <!-- Language Switch Script -->
<script>
  document.addEventListener('DOMContentLoaded', function () {
    const languageSelectors = document.querySelectorAll('.languageSwitcher');
    const arabicNav = document.querySelector('.my-navbarAr');
    const englishNav = document.querySelector('.my-navbarEn');
    const warpa = document.querySelectorAll('.wrap-ar');
    const warpe = document.querySelectorAll('.wrap-en');

    // قراءة اللغة المحفوظة من localStorage أو افتراضي عربي
    const savedLang = localStorage.getItem('selectedLang') || 'ar';

    // قراءة اللغة الحالية من الرابط
    const currentPath = window.location.pathname;
    const currentLang = currentPath.startsWith('/en') ? 'en' :
                        currentPath.startsWith('/ar') ? 'ar' : 'ar'; // الافتراضي عربي

    // تعيين قيمة اللغة الحالية في الـ dropdown
    languageSelectors.forEach(select => {
      select.value = savedLang;
    });

    function updatePageLang(lang) {
      localStorage.setItem('selectedLang', lang);

      languageSelectors.forEach(select => {
        select.value = lang;
      });

      if (lang === 'en') {
        englishNav.style.display = 'block';
        arabicNav.style.display = 'none';
        warpa.forEach(el => el.style.display = 'none');
        warpe.forEach(el => el.style.display = 'block');

        if (currentLang !== 'en') {
          window.location.href = '/website/lang/en_US?redirect=' + window.location.pathname;
        }
      } else {
        englishNav.style.display = 'none';
        arabicNav.style.display = 'block';
        warpa.forEach(el => el.style.display = 'block');
        warpe.forEach(el => el.style.display = 'none');

       if (currentLang !== 'ar' &amp;&amp; window.location.pathname !== '/en/web/login') {
  window.location.href = '/website/lang/ar_SY?redirect=' + window.location.pathname;
}
      }
    }

    // تشغيل فقط لو savedLang مختلفة عن currentLang
    if (savedLang !== currentLang) {
      updatePageLang(savedLang);
    }

    // عند تغيير اللغة يدويًا
    languageSelectors.forEach(select => {
      select.addEventListener('change', () => {
        updatePageLang(select.value);
      });
    });
  });
</script>

